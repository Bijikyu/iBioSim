#!/bin/sh
#
# run-time wrapper for BioSim gui
export LD_LIBRARY_PATH=$BIOSIM/gui/lib/libsbml/lib:$LD_LIBRARY_PATH
exec java -classpath $BIOSIM/gui/dist/classes:$BIOSIM/gui/lib/jcommon-1.0.10.jar:$BIOSIM/gui/lib/jfreechart-1.0.6.jar:$BIOSIM/gui/lib/itext-2.0.3.jar:$BIOSIM/gui/lib/batik-awt-util.jar:$BIOSIM/gui/lib/batik-dom.jar:$BIOSIM/gui/lib/batik-ext.jar:$BIOSIM/gui/lib/batik-svggen.jar:$BIOSIM/gui/lib/batik-util.jar:$BIOSIM/gui/lib/batik-xml.jar:$BIOSIM/gui/lib/epsgraphics.jar:$BIOSIM/gui/lib/libsbml/lib/libsbmlj.jar biomodelsim.core.gui.BioSim
